      "source": [
        "import numpy as np\n",
        "\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "\n",
        "# 行列Aと行列Bの内積を計算\n",
        "result = np.dot(A, B)\n",
        "\n",
        "# 最大値を求める\n",
        "max_value = np.max(result)\n",
        "\n",
        "print(\"行列Aと行列Bの内積:\")\n",
        "print(result)\n",
        "print(\"最大値:\", max_value)\n",
        "\n"
      ],
